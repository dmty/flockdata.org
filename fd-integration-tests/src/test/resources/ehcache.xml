    <cache name="entityLog"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="companyFortress"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="fortressName"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="systemUsers"
           maxElementsInMemory="75"
           eternal="false"
           timeToIdleSeconds="120"
           timeToLiveSeconds="320"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="fortressUser"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LRU"/>

</ehcache>
            memoryStoreEvictionPolicy="LRU"/>

    <cache name="companyTag"
           maxElementsInMemory="400"
           eternal="true"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="companyEvent"
           maxElementsInMemory="100"
           eternal="true"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="companyTagManager"
           maxElementsInMemory="10"
           eternal="true"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="companyDocType"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="companySchemaTag"
           maxElementsInMemory="200"
           eternal="true"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="metaKey"
           maxElementsInMemory="200"
           eternal="false"
           timeToIdleSeconds="120"
           timeToLiveSeconds="120"
           memoryStoreEvictionPolicy="LFU"/>

    <cache name="callerKey"
           maxElementsInMemory="400"

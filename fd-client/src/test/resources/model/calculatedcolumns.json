{
  "contentType": "csv",
  "tagOrEntity": "entity",
  "fortressName": "hr",
  "documentName": "Payroll",
  "delimiter": ",",
  "header": true,
  "content": {
    "BulkHours": {
      "dataType": "double"
    },
    "ScheduledHours": {
      "dataType": "double"
    },
    "Hours": {
      "dataType": "double"
    },
    "VarianceHours": {
      "dataType": "double",
      "value": "(#data['ScheduledHours']-(#data['Hours'])+#data['BulkHours'])",
      "target": "VarianceHours"
    },
    "WorkHours": {
      "dataType": "double",
      "target": "WorkHours",
      "value":  "#data['Hours']+#data['BulkHours']",
      "properties": [
        {
          "source":  "WorkHours",
          "target": "value"
        }
      ]
    }
  }
}
